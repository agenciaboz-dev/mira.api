generator client {
    provider = "prisma-client-js" 
    previewFeatures = ["fullTextSearch", "fullTextIndex"]
}

datasource db {
    provider = "mysql"
    url      = env("DATABASE_URL")
}

model users {
    id          Int         @id @default(autoincrement())
    username    String      @unique
    email       String      @unique
    password    String
    name        String
    adm         Boolean     @default(false)
}